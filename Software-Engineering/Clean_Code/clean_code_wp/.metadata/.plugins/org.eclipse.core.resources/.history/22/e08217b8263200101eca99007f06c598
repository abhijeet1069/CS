package com.satyam.interfacesAsTypes.animalPOJO;

import com.satyam.interfacesAsTypes.animalTypes.Fish;

public class PufferFish implements Fish{

	@Override
	public String getName() {
		return "Puffer Fish";
	}

	@Override
	public String fact() {
		return "Puffer fish can puff themselves when they sense danger";
	}
	
	@Override
	public String toString() {
		return "Elephant [Name= " + getName() + ", Fact= " + fact() + "]";
	}

}
