package com.satyam.interfacesAsTypes;

import java.util.ArrayList;
import java.util.List;

import com.satyam.interfacesAsTypes.animalTypes.Bird;
import com.satyam.interfacesAsTypes.animalTypes.Fish;
import com.satyam.interfacesAsTypes.animalTypes.Mammal;

public class Zoo {
	List<Bird> birds;
	List<Fish> fishes;
	List<Mammal> mammals;
	
	Zoo(){
		birds = new ArrayList<Bird>();
		fishes = new ArrayList<Fish>();
		mammals = new ArrayList<Mammal>();
	}
}
